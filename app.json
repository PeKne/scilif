{
  "expo": {
    "name": "SunFibre Native",
    "slug": "scilif",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./resources/images/favicon.png",
    "splash": {
      "image": "./resources/images/logo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pekne.scilif"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.pekne.scilif"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "@config-plugins/react-native-ble-plx"
    ]
  }
}
